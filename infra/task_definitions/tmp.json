{
  "containerDefinitions": [
    {
      "name": "venus-tmp",
      "image": "443815146287.dkr.ecr.ap-northeast-1.amazonaws.com/venus-ecr-repository-main:tmp02",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": [],
      "command": [
        "rails",
        "s",
        "-b",
        "0.0.0.0"
      ],
      "environment": [
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "DATABASE_URL",
          "value": "mysql2://root:root@db/venus"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/venus-test",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "tmp"
        }
      }
    }
  ],
  "family": "venus-test-tmp",
  "taskRoleArn": "arn:aws:iam::443815146287:role/venus-test-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::443815146287:role/venus-test-ecs-task-execution-role",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
